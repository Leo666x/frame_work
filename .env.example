# Power AI Framework 环境配置示例
# 复制此文件为 .env 并填入实际值

# 基础配置
IP_ADDR=127.0.0.1
PORT=40000

# PostgreSQL 数据库配置
POWER_AI_POSTGRES_HOST=127.0.0.1
POWER_AI_POSTGRES_PORT=39002
POWER_AI_POSTGRES_USERNAME=postgres
POWER_AI_POSTGRES_PASSWORD=your_postgres_password_here
POWER_AI_POSTGRES_DATABASE=powerai

# Redis 配置
POWER_AI_REDIS_HOST=127.0.0.1
POWER_AI_REDIS_PORT=39009
POWER_AI_REDIS_PASSWORD=your_redis_password_here

# MinIO 对象存储配置
POWER_AI_MINIO_HOST=127.0.0.1
POWER_AI_MINIO_API_PORT=39004
POWER_AI_MINIO_USERNAME=your_minio_username_here
POWER_AI_MINIO_PASSWORD=your_minio_password_here

# Weaviate 向量数据库配置
POWER_AI_WEAVIATE_HOST=127.0.0.1
POWER_AI_WEAVIATE_HTTP_PORT=39006
POWER_AI_WEAVIATE_API_KEY=your_weaviate_api_key_here

# Milvus 向量数据库配置
POWER_AI_MILVUS_HOST=127.0.0.1
POWER_AI_MILVUS_PORT=39008
POWER_AI_MILVUS_USERNAME=your_milvus_username_here
POWER_AI_MILVUS_PASSWORD=your_milvus_password_here

# ETCD 配置
POWER_AI_ETCD_HOST=127.0.0.1
POWER_AI_ETCD_PORT=39001
POWER_AI_ETCD_ENDPOINTS=127.0.0.1:39001

# HTTP 客户端配置
HTTP_CLIENT_TIMEOUT=600
HTTP_CLIENT_COMPRESSED=false
HTTP_CLIENT_HANDSHAKE_TIMEOUT=600
HTTP_CLIENT_RESPONSE_TIMEOUT=600

# LLM HTTP 客户端配置
HTTP_LLM_CLIENT_TIMEOUT=600
HTTP_LLM_CLIENT_COMPRESSED=false
HTTP_LLM_CLIENT_HANDSHAKE_TIMEOUT=600
HTTP_LLM_CLIENT_RESPONSE_TIMEOUT=600